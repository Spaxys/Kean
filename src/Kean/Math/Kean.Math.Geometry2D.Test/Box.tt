           Expect(this.box0.Equals(this.box0 as object), Is.True);
           Expect(this.box0 == this.box0, Is.True);
           Expect(this.box0 != this.box1, Is.True);
           Expect(this.box0 == box, Is.False);
           Expect(box == box, Is.True);
           Expect(box == this.box0, Is.False);
       }

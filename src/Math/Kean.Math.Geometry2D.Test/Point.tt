        [Test]
        public void Casts()
        {
            // integer - double
            {
                Target.Integer.<#= @name #> integer = new Target.Integer.<#= @name #>(10, 20);
                Target.Double.<#= @name #> @double = integer;
                Expect(@double.X, Is.EqualTo(10));
                Expect(@double.Y, Is.EqualTo(20));
                Expect((Target.Integer.<#= @name #>)@double, Is.EqualTo(integer));
            }
            // single - double
            {
                Target.Single.<#= @name #> single = new Target.Single.<#= @name #>(10, 20);
                Target.Double.<#= @name #> @double = single;
                Expect(@double.X, Is.EqualTo(10));
                Expect(@double.Y, Is.EqualTo(20));
                Expect((Target.Single.<#= @name #>)@double, Is.EqualTo(single));
            }
        }
		[Test]
        public void StringCasts()
        {
            string textFromValue = new Target.Double.<#= @name #>(10, 20);
            Expect(textFromValue, Is.EqualTo("10, 20"));
            Target.Double.<#= @name #> @integerFromText = "10, 20";
            Expect(@integerFromText.X, Is.EqualTo(10));
            Expect(@integerFromText.Y, Is.EqualTo(20));
        }

using System;
using NUnit.Framework;
using NUnit.Framework.SyntaxHelpers;
using Target = Kean.Math.Geometry2D;
using Kean.Core.Extension;

namespace <#= @namespace #>.<#= @namespace1 #>
{
    [TestFixture]
    public class <#= @name #> :
        Kean.Test.Fixture<<#= @name #>>
    {
        string prefix = "<#= @namespace #>.<#= @namespace1 #>.<#= @name #>";
        float Precision { get { return 1e-4f; } }
        Target.<#= @namespace1 #>.<#= @name #> CastFromString(string value)
        {
            return value;
        }
        string CastToString(Target.<#= @namespace1 #>.<#= @name #> value)
        {
            return value;
        }
        Target.<#= @namespace1 #>.<#= @name #> vector0 = new Target.<#= @namespace1 #>.<#= @name #>((<#= @value1 #>)22.221f, (<#= @value1 #>)-3.1f);
        Target.<#= @namespace1 #>.<#= @name #> vector1 = new Target.<#= @namespace1 #>.<#= @name #>((<#= @value1 #>)12.221f, (<#= @value1 #>)13.1f);
        Target.<#= @namespace1 #>.<#= @name #> vector2 = new Target.<#= @namespace1 #>.<#= @name #>((<#= @value1 #>)34.442f, (<#= @value1 #>)10.0f);
        Target.<#= @namespace1 #>.<#= @name #> vector3 = new Target.<#= @namespace1 #>.<#= @name #>((<#= @value1 #>)10, (<#= @value1 #>)20);

        protected override void Run()
        {
            this.Run(
                this.Polar0,
                this.Polar1,
                this.Polar2,
                this.Polar3,
                this.Polar4,
                this.Polar5,
                this.Angles,
                this.Casts,
                this.StringCasts
                );
        }
        #region Equality
        [Test]
        public void Equality()
        {
            Target.<#= @namespace1 #>.<#= @name #> point = new Target.<#= @namespace1 #>.<#= @name #>();
            Expect(this.vector0, Is.EqualTo(this.vector0));
            Expect(this.vector0.Equals(this.vector0 as object), Is.True);
            Expect(this.vector0 == this.vector0, Is.True);
            Expect(this.vector0 != this.vector1, Is.True);
            Expect(this.vector0 == point, Is.False);
            Expect(point == point, Is.True);
            Expect(point == this.vector0, Is.False);
        }
        #endregion
        #region Arithmetic
        [Test]
        public void Addition()
        {
            Expect((this.vector0 + this.vector1).X, Is.EqualTo(this.vector2.X).Within(this.Precision));
            Expect((this.vector0 + this.vector1).Y, Is.EqualTo(this.vector2.Y).Within(this.Precision));
        }
        [Test]
        public void Subtraction()
        {
            Expect(this.vector0 - this.vector0, Is.EqualTo(new Target.<#= @namespace1 #>.<#= @name #>()));
        }
        [Test]
        public void ScalarMultiplication()
        {
            Expect((-1) * this.vector0, Is.EqualTo(-this.vector0));
        }
        [Test]
        public void ScalarDivision()
        {
            Expect(this.vector0 / (-1), Is.EqualTo(-this.vector0));
        }
        #endregion
        #region Hash Code
        [Test]
        public void Hash()
        {
            Expect(this.vector0.Hash(), Is.Not.EqualTo(0));
        }
        #endregion

        [Test]
        public void GetValues()
        {
            Expect(this.vector0.X, Is.EqualTo((<#= @value1 #>)(22.221)).Within(this.Precision), this.prefix + "GetValues.0");
            Expect(this.vector0.Y, Is.EqualTo((<#= @value1 #>)(-3.1)).Within(this.Precision), this.prefix + "GetValues.1");
        }
        [Test]
        public void Swap()
        {
            Target.<#= @namespace1 #>.<#= @name #> result = this.vector0.Swap();
            Expect(result.X, Is.EqualTo(this.vector0.Y), this.prefix + "Swap.0");
            Expect(result.Y, Is.EqualTo(this.vector0.X), this.prefix + "Swap.1");
        }
        [Test]
        public void Casting()
        {
            string value = "10, 20";
            Expect(this.CastToString(this.vector3), Is.EqualTo(value), this.prefix + "Casting.0");
            Expect(this.CastFromString(value), Is.EqualTo(this.vector3), this.prefix + "Casting.1");
        }
        [Test]
        public void CastingNull()
        {
            string value = null;
            Target.<#= @namespace1 #>.<#= @name #> point = null;
            Expect(this.CastToString(point), Is.EqualTo(value), this.prefix + "CastingNull.0");
            Expect(this.CastFromString(value), Is.EqualTo(point), this.prefix + "CastingNull.1");
        }

        #region Polar Representation
        [Test]
        public void Polar0()
        {
            Target.<#= @namespace1 #>.<#= @name #> point = new Target.<#= @namespace1 #>.<#= @name #>();
            Expect(point.Norm, Is.EqualTo(0));
            Expect(point.Azimuth, Is.EqualTo(0));
        }
        [Test]
        public void Polar1()
        {
            Target.<#= @namespace1 #>.<#= @name #> point = new Target.<#= @namespace1 #>.<#= @name #>(1, 0);
            Expect(point.Norm, Is.EqualTo(1));
            Expect(point.Azimuth, Is.EqualTo(0));
        }
        [Test]
        public void Polar2()
        {
            Target.<#= @namespace1 #>.<#= @name #> point = new Target.<#= @namespace1 #>.<#= @name #>(0, 1);
            Expect(point.Norm, Is.EqualTo(1));
            Expect(point.Azimuth, Is.EqualTo(Kean.Math.<#= @namespace1 #>.ToRadians(90)));
        }
        [Test]
        public void Polar3()
        {
            Target.<#= @namespace1 #>.<#= @name #> point = new Target.<#= @namespace1 #>.<#= @name #>(0, -5);
            Expect(point.Norm, Is.EqualTo(5));
            Expect(point.Azimuth, Is.EqualTo(Kean.Math.<#= @namespace1 #>.ToRadians(-90)));
        }
        [Test]
        public void Polar4()
        {
            Target.<#= @namespace1 #>.<#= @name #> point = new Target.<#= @namespace1 #>.<#= @name #>(-1, 0);
            Expect(point.Norm, Is.EqualTo(1));
            Expect(point.Azimuth, Is.EqualTo(Kean.Math.<#= @namespace1 #>.ToRadians(180)));
        }
        [Test]
        public void Polar5()
        {
            Target.<#= @namespace1 #>.<#= @name #> point = new Target.<#= @namespace1 #>.<#= @name #>(-3, 0);
            <#= @value1 #> radius = point.Norm;
            <#= @value1 #> azimuth = point.Azimuth;
            Target.<#= @namespace1 #>.<#= @name #> point2 = Target.<#= @namespace1 #>.<#= @name #>.Polar(radius, azimuth);
            Expect(point.Distance(point2), Is.EqualTo(0).Within(this.Precision));
        }
        #endregion
        [Test]
        public void Angles()
        {
            Expect(Target.Single.<#= @name #>.BasisX.Angle(Target.Single.<#= @name #>.BasisX), Is.EqualTo(0).Within(this.Precision));
            Expect(Target.Single.<#= @name #>.BasisX.Angle(Target.Single.<#= @name #>.BasisY), Is.EqualTo(Kean.Math.Single.Pi / 2).Within(this.Precision));
            Expect(Target.Single.<#= @name #>.BasisX.Angle(-Target.Single.<#= @name #>.BasisY), Is.EqualTo(-Kean.Math.Single.Pi / 2).Within(this.Precision));
            Expect(Target.Single.<#= @name #>.BasisX.Angle(-Target.Single.<#= @name #>.BasisX), Is.EqualTo(Kean.Math.Single.Pi).Within(this.Precision));
        }
<#+
    }  
